deviceinfo_format_version="0"
deviceinfo_name="Nothing CMF Phone 1"
deviceinfo_manufacturer="Nothing"
deviceinfo_codename="nothing-tetris"
deviceinfo_year="2024"
deviceinfo_dtb="mt6878-nothing-tetris"
deviceinfo_arch="aarch64"

# Device related
deviceinfo_chassis="handset"
deviceinfo_external_storage="false"
deviceinfo_rootfs_image_sector_size="4096"

# Bootloader related
# Note: MT6878 devices CANNOT boot with stock lk to mainline. The stock
# lk messes up the mainline device tree and will not boot. As such,
# creating the mainline boot.img is done manually for now.
# Packaging with uniLoader is not supported right now in postmarketOS.
deviceinfo_flash_method="none"
